C: Client;
S: Server;
B: Backend;
->C: Hit;
..: Server query 
    [line.corner=round]
{
  C==C: Generate\nrequest
      [strong, line.corner=note];
  C=>S: Request;
  S..B: Server gets info
      [color=lgray, 
       line.corner=bevel]
  {
    S>>B: Query;
    S<<B: Response;
  };
  C<=S: Reply;
};

